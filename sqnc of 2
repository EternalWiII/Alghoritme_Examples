#include <iostream>
#include <limits>

/*
	10 - 2
	100 - 4
	1000 - 8
	10000 - 16
	(2 & (2 - 1)) - 10 & 1 == 0 or 2 & 1 == 0
*/
void is_2_seq(const long long int n) {
	if((n & (n-1)) == 0){
		std::cout << n << std::endl;
	} else {
		return;
	}
}

